<div class="container card--featured">
    <h2>Contacto</h2>
    <form class="form" action="/contacto" method="post">
        <div class="form__group"><label class="form__label" for="txtnombre">Nombre: </label><input class="form__input"
                type="text" name="txtnombre" id="txtnombre"></div>
        <div><label class="form__label" for="txtEmail">Email: </label><input class="form__input" type="email"
                name="txtEmail" id="txtEmail"></div>
        <div class="form__group"><label class="form__label" for="txtMsj">Mensaje: </label><input class="form__input"
                type="text" name="txtMsj" id="txtMsj"></div>
        <button class="button button--primary" type="submit">Enviar</button>
    </form>
</div>