<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Nosotros</title>
    <link rel="stylesheet" href="./css/style.css" />
  </head>
  <body>
    <h1>Productos</h1>
    <table>
        <thead>
            <tr>
                <th>Titulo</th>
                <th>Precio</th>
                <th>Categoria</th>
                <th>Descripci√≥n</th>
            </tr>
        </thead>
        <tbody>
        <% productos.forEach( p => {%>
          <tr>
            <td><%= p.title %></td>
            <td><%= p.price %></td>
            <td><%= p.category %></td>
            <td><%= p.description %></td>
          </tr>
       <% }) %>
        </tbody>
    </table>
  </body>
</html>
